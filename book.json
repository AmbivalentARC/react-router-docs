{
  "gitbook": "3.0.0-pre.5",
  "root": "./docs",
  "plugins": [
    "prism",
    "-highlight",
    "theme-official",
    "edit-link",
    "github",
    "anker-enable",
    "jsfiddle"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/knowbody/react-router-docs/tree/master/docs",
      "label": ""
    },
    "github": {
      "url": "https://github.com/knowbody/react-router-docs/tree/master/docs"
    },
    "jsfiddle":{
      "type": "frame",
      "tabs": ["result"],
      "height": "500",
      "width": "100%"
    }
  }
}
